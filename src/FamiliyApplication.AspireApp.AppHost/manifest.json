{
  "$schema": "https://json.schemastore.org/aspire-8.0.json",
  "resources": {
    "familyapp-kv": {
      "type": "azure.bicep.v0",
      "connectionString": "{familyapp-kv.outputs.vaultUri}",
      "path": "familyapp-kv.module.bicep"
    },
    "webfrontend": {
      "type": "project.v0",
      "path": "../FamiliyApplication.AspireApp.Web/FamiliyApplication.AspireApp.Web.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{webfrontend.bindings.http.targetPort}",
        "ConnectionStrings__familyapp-kv": "{familyapp-kv.connectionString}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        }
      }
    },
    "familyapp-kv-roles": {
      "type": "azure.bicep.v0",
      "path": "familyapp-kv-roles.module.bicep",
      "params": {
        "familyapp_kv_outputs_name": "{familyapp-kv.outputs.name}",
        "principalType": "",
        "principalId": ""
      }
    }
  }
}